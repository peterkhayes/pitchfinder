  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^6.0.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "expect": "^1.16.0",
    "mocha": "^2.4.5",
